    "ngx-pipes": "^2.7.3",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.1.0",
    "rxjs": "^6.5.4",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tslint": "~6.1.0",
    "typescript": "4.0.5",
    "typescript-tools": "^0.3.1"
  },
  "repository": {}
}
